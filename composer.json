{
    "name": "ssofy/php-sdk",
    "type": "library",
    "description": "Official SSOfy PHP SDK for instant and efficient API access.",
    "keywords": [
        "ssofy",
        "sso",
        "login",
        "auth",
        "authentication",
        "oauth",
        "oauth2",
        "sdk"
    ],
    "homepage": "https://www.ssofy.com",
    "license": "MIT",
    "authors": [
        {
            "name": "SSOfy",
            "email": "support@ssofy.com"
        },
        {
            "name": "Cubelet Ltd.",
            "email": "info@cubelet.co.uk"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-json": "*",
        "league/oauth2-client": "^2.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "scripts": {
        "test": "phpunit --exclude skip",
        "cs-fixer": "php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
